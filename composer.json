{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "easycorp/easyadmin-bundle": "^2.3",
        "gos/web-socket-bundle": "^3.1",
        "knplabs/knp-paginator-bundle": "^5.2",
        "sensio/framework-extra-bundle": "^5.2",
        "symfony/asset": "5.1.0",
        "symfony/console": "5.1.0",
        "symfony/dotenv": "5.1.*",
        "symfony/event-dispatcher": "5.1.0",
        "symfony/flex": "^1.1",
        "symfony/form": "5.1.0",
        "symfony/framework-bundle": "5.1.0",
        "symfony/messenger": "5.1.0",
        "symfony/mime": "5.1.0",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "^1.0",
        "symfony/process": "5.1.0",
        "symfony/security-bundle": "5.1.*",
        "symfony/serializer": "5.1.0",
        "symfony/serializer-pack": "1.0.3",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "5.1.0",
        "symfony/twig-pack": "^1.0",
        "symfony/validator": "5.1.*",
        "symfony/var-dumper": "5.1.*",
        "symfony/web-link": "5.1.*",
        "symfony/webpack-encore-bundle": "^1.0",
        "symfony/yaml": "5.1.*"
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.19",
        "doctrine/doctrine-fixtures-bundle": "^3.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true",
            "require": "5.1.0"
        }
    }
}
